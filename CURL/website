    <script>
  (function(window) {
    function appendOffersAndUpdateEventsConfig(eventsConfig, offerIdsToAppend) {
      window.aureusContext = window.aureusContext || {};
      window.aureusContext.offerIds = window.aureusContext.offerIds || [];
      window.aureusContext.offerIds.push(offerIdsToAppend);
      eventsConfig.EI = window.aureusContext.offerIds.toString();
    }

    var generatedOfferIds = ["9glmSsIQ"];
    if ("undefined" !== typeof window.dlApi) {
      appendOffersAndUpdateEventsConfig(window.dlApi.kropka, generatedOfferIds);
    } else if ("undefined" !== typeof window.onetAds) {
      appendOffersAndUpdateEventsConfig(window.onetAds.kropka, generatedOfferIds);
    } else {
      console.warn("Couldn't update Aureus offers - no Kropka context present.");
    }
  })(window);
</script>
